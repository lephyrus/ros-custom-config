<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="personal-remote" fetch="https://github.com/lephyrus/" revision="11.0" />
  <remote name="grapheneos" fetch="https://github.com/grapheneos/" revision="11" />
  <remote name="seedvault-app" fetch="https://github.com/seedvault-app/" revision="master" />

  <!-- this custom repo - do not change path -->
  <project path="vendor/custom" name="ros-custom-config" remote="personal-remote" />

  <!-- microg repo from rattlesnakeos -->
  <project path="platform/prebuilts/microg" name="microg" remote="rattlesnakeos" clone-depth="1" />

  <!-- vanadium prebuilt -->
  <project path="external/vanadium" name="platform_external_vanadium" remote="grapheneos" clone-depth="1" />
  <remove-project name="platform/external/chromium-webview" />
  <remove-project name="platform/packages/apps/Browser2" />
  <remove-project name="platform/packages/apps/QuickSearchBox" />

  <!-- seedvault pinned to a specific revision -->
  <project path="external/seedvault" name="seedvault" remote="seedvault-app" revision="e38f958f9c1fade27fcb31f2e9b273450978f70a" clone-depth="1" />

  <!-- AuroraStore, AuroraServices -->
  <project path="external/aurora" name="ros-aurora" remote="personal-remote" clone-depth="1" />

</manifest>
